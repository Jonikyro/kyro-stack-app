{
	"name": "kyro-stack-app",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"dev-sync": "concurrently \"vite --port 5174\" \"browser-sync start --proxy \"localhost:5174/\" --port 5173 --no-open\"",
		"build:translations": "i18next 'src/**/*.{ts, tsx}' --config i18next.config.cjs",
		"build:icons": "tsx ./src/icons/build-icons.ts",
		"build": "tsc -b && vite build",
		"lint": "eslint . --report-unused-disable-directives --max-warnings 0",
		"ladle": "npx ladle serve",
		"format": "prettier --write ./src",
		"preview:icons": "vite --config vite.icon-preview.config.ts",
		"depcheck": "npx depcheck",
		"typecheck": "tsc -b ./tsconfig.app.json ./tsconfig.node.json"
	},
	"dependencies": {
		"@internationalized/date": "^3.5.6",
		"@ladle/react": "^5.0.3",
		"@tanstack/react-query": "^5.90.21",
		"@tanstack/react-query-devtools": "^5.91.3",
		"@tanstack/react-router": "^1.159.10",
		"@tanstack/react-router-devtools": "^1.159.10",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.1",
		"downshift": "^9.0.8",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.5",
		"fuzzysort": "^3.1.0",
		"i18next": "^23.11.5",
		"ky": "^1.4.0",
		"react": "19.2.4",
		"react-aria": "^3.46.0",
		"react-dom": "19.2.4",
		"react-hook-form": "^7.71.1",
		"react-stately": "^3.44.0",
		"tailwind-merge": "^3.3.1"
	},
	"devDependencies": {
		"@csstools/postcss-global-data": "^3.0.0",
		"@eslint/compat": "^2.0.2",
		"@eslint/eslintrc": "^3.3.3",
		"@eslint/js": "^9.17.0",
		"@tailwindcss/vite": "^4.1.18",
		"@tanstack/eslint-plugin-query": "^5.91.4",
		"@tanstack/eslint-plugin-router": "^1.155.0",
		"@tanstack/router-plugin": "^1.159.12",
		"@types/node": "^20.14.9",
		"@types/react": "19.2.14",
		"@types/react-dom": "19.2.3",
		"@types/svg-sprite": "^0.0.39",
		"@typescript-eslint/eslint-plugin": "^8.55.0",
		"@typescript-eslint/parser": "^8.55.0",
		"@vitejs/plugin-react": "^5.1.4",
		"autoprefixer": "^10.4.19",
		"concurrently": "^9.0.1",
		"consola": "^3.2.3",
		"eslint": "^9.17.0",
		"eslint-plugin-react-hooks": "^7.0.1",
		"eslint-plugin-react-refresh": "^0.5.0",
		"i18next-parser": "^9.0.0",
		"postcss": "^8.4.47",
		"postcss-custom-media": "^11.0.2",
		"postcss-import": "^16.1.0",
		"postcss-nested": "^7.0.2",
		"postcss-nesting": "^13.0.0",
		"prettier": "^3.3.2",
		"prettier-plugin-tailwindcss": "^0.6.12",
		"svg-sprite": "^2.0.4",
		"tailwindcss": "^4.1.10",
		"tsx": "^4.19.1",
		"typescript": "^5.8.3",
		"unplugin-inject-preload": "^3.0.0",
		"vite": "^7.3.1"
	}
}
