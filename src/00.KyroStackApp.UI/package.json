		"@tanstack/react-router": "^1.121.34",
		"@tanstack/react-router-devtools": "^1.121.34",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.1",
		"downshift": "^9.0.8",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-prettier": "^5.1.3",
		"fuzzysort": "^3.1.0",
		"i18next": "^23.11.5",
		"ky": "^1.4.0",
